{% extends 'base.html.twig' %}

{% block title %}Symfony blog{% endblock %}

{% block body %}
    {% if blog %}
       <div>
            <h1>{{ blog.title }}</h1>
            <p class="h4">Author: {{ blog.author.name }}</p>
            <p>{{ blog.body }}</p>
       </div>
    {% else %}
        <p>Could not find blog</p>
    {% endif %}
{% endblock %}